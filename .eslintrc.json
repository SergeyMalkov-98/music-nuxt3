{
  "root": true,
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [ "@nuxtjs/eslint-config-typescript", "plugin:prettier/recommended" ],
  "overrides": [
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {   
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "plugins": [   
  ],
  "rules": {
    "no-unused-vars": "off",
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "react/react-in-jsx-scope": "off",
    "linebreak-style": 0,
    "no-var": "error",
    "prefer-const": "error",
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}